/**
 * Implements hook_ctools_plugin_directory().
 */
function restful_panels_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'restful' || $owner == 'panels') {
    return 'plugins/' . $plugin_type;
  }

  return NULL;
}
